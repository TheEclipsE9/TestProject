@using TestProjectWeb.Models
@model List<UserViewModel>

<div>
    <div>
        All users:
    </div>
    <div>          
        @foreach (var user in Model)
        {
            <div> 
                <a asp-controller="User" asp-action="UserProfile" asp-route-id="@user.Id">@user.Name</a>
            </div>
        }   
    </div>  
</div>